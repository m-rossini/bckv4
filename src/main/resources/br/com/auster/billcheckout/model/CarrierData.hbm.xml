<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"etc/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class lazy="false"
		name="br.com.auster.billcheckout.model.CarrierData"
		table="BCK_CARRIER_DATA">
		<id name="uid"
			column="OBJID"
			type="long"
			unsaved-value="0">
			<generator class="sequence">
				<param name="sequence">bck_dimensions_uid</param>
			</generator>
		</id>
		<property
			name="fullName"
			type="string"
			update="true"
			insert="true"
			column="CARRIER_FULL_NAME" 
			length="128"
			not-null="false"/>
		<property
			name="taxId"
			type="string"
			update="true"
			insert="true"
			column="CARRIER_TAX_ID" 
			length="24"
			not-null="false"/>
		<property
			name="stateEnrollNumber"
			type="string"
			update="true"
			insert="true"
			column="CARRIER_STATE_ENROLL_NBR"
			length="24"
			not-null="false"/>
		<property
			name="cityEnrollNumber"
			type="string"
			update="true"
			insert="true"
			column="CARRIER_CITY_ENROLL_NBR"
			length="24"
			not-null="false"/>
		<property
			name="addressStreet"
			type="string"
			update="true"
			insert="true"
			column="ADDR_STREET"
			length="128"
			not-null="false"/>
		<property
			name="addressNumber"
			type="string"
			update="true"
			insert="true"
			column="ADDR_NUMBER"
			length="8"
			not-null="false"/>
		<property
			name="addressComplement"
			type="string"
			update="true"
			insert="true"
			column="ADDR_COMPLEMENT"
			length="32"
			not-null="false"/>
		<property
			name="addressZip"
			type="string"
			update="true"
			insert="true"
			column="ADDR_ZIP"
			length="16"
			not-null="false"/>
		<property
			name="addressCity"
			type="string"
			update="true"
			insert="true"
			column="ADDR_CITY"
			length="32"
			not-null="false"/>
		<property
			name="addressWeb"
			type="string"
			update="true"
			insert="true"
			column="ADDR_WEB"
			length="64"
			not-null="false"/>
		<property
			name="addressEmail"
			type="string"
			update="true"
			insert="true"
			column="ADDR_EMAIL"
			length="64"
			not-null="false"/>
		<many-to-one
			name="carrierDimension"
			column="CARRIER_DM_UID"
			not-null="true"/>
		<property
			name="custom1"
			type="string"
			update="true"
			insert="true"
			column="CUSTOM_1"
			length="30"
			not-null="false"/>
		<property
			name="custom2"
			type="string"
			update="true"
			insert="true"
			column="CUSTOM_2"
			length="30"
			not-null="false"/>
		<property
			name="custom3"
			type="string"
			update="true"
			insert="true"
			column="CUSTOM_3"
			length="30"
			not-null="false"/>
	</class>
</hibernate-mapping>