<?xml version="1.0" encoding="ISO-8859-1"?>

<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "etc/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

    <class lazy="false"
        name="br.com.auster.billcheckout.model.TaxType"
        table="BCK_TAX_TYPE">

        <id name="uid"
            column="OBJID"
            type="long"
            unsaved-value="0">
            <generator class="sequence">
                <param name="sequence">bck_dimensions_uid</param>
            </generator>
        </id>

        <property
            name="taxCode"
            type="string"
            update="true"
            insert="true"
            column="TAX_CODE" 
            length="10"
            not-null="true"/>

        <property
            name="taxName"
            type="string"
            update="true"
            insert="true"
            column="TAX_NAME" 
            length="64"
            not-null="true"/>

        <property
            name="custom1"
            type="string"
            update="true"
            insert="true"
            column="CUSTOM_1"
            length="30"
            not-null="false"/>

        <property
            name="custom2"
            type="string"
            update="true"
            insert="true"
            column="CUSTOM_2"
            length="30"
            not-null="false"/>

        <property
            name="custom3"
            type="string"
            update="true"
            insert="true"
            column="CUSTOM_3"
            length="30"
            not-null="false"/>

   </class>

</hibernate-mapping>
